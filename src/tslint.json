{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "no-inferrable-types": false,
    "semicolon": false,
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
